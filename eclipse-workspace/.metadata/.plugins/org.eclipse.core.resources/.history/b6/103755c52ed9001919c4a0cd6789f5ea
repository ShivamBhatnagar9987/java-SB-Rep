import java.util.concurrent.Callable;

public class CallableAdderThread2 implements Callable<int[]> {
	int op1, op2;

	public CallableAdderThread2(int op1, int op2) {
		this.op1 = op1;
		this.op2 = op2;
	}

	public int[] call() {

		return init();
	}

	public int[] init() {
		int[] ar = new int[] { op1, op2 };
		return ar;
	}

	public int[] process(int[] ar) {
		return ar;
	}
}

import java.util.concurrent.Callable;

public class CallableAdder implements Callable<Integer[]> {
	Integer op1, op2;

	public CallableAdder(int op1, int op2) {
		this.op1 = op1;
		this.op2 = op2;
	}

	public Integer[] call() {

		Integer[] ar = new Integer[] { 10, 20 };
		return init(ar);
	}

	public Integer[] init(Integer[] ar) {
		return ar;
	}
}
